{
    "name": "rask/caphpe",
    "description": "A volatile in-memory key-value storage written in PHP",
    "authors": [
        {
            "name": "Otto Rask",
            "email": "ojrask@gmail.com"
        }
    ],

    "autoload": {
        "psr-4": {
            "Caphpe\\": "src/"
        }
    },

    "autoload-dev": {
        "psr-4": {
            "Caphpe\\Tests\\": "tests/"
        }
    },

    "require": {
        "php": ">=7.2",
        "ext-sockets": "*",
        "ext-mbstring": "*",
        "react/react": "^0.4.2"
    },

    "require-dev": {
        "roave/security-advisories": "dev-master",
        "phpunit/phpunit": "8.*"
    }
}
